    "astro": "^4.8.6",
    "butterchurn": "3.0.0-beta.4",
    "butterchurn-presets": "3.0.0-beta.4",
    "gsap": "^3.12.5",
    "obs-websocket-js": "^5.0.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "workbox-window": "^7.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.7.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@types/spotify-web-playback-sdk": "^0.1.19",
